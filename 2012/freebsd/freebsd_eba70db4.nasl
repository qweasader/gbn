vuln = FALSE;
txt = "";

bver = portver(pkg:"dropbear");
if(!isnull(bver) && revcomp(a:bver, b:"0.51")>=0 && revcomp(a:bver, b:"2012.55")<0) {
  txt += "Package dropbear version " + bver + " is installed which is known to be vulnerable.\n";
  vuln = TRUE;
}

if(vuln) {
  security_message(data:txt);
} else if (__pkg_match) {
  exit(99);
