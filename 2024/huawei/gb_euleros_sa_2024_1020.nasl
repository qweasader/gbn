  script_tag(name:"severity_origin", value:"NVD");
  script_tag(name:"severity_date", value:"2023-10-10 13:44:00 +0000 (Tue, 10 Oct 2023)");

  script_name("Huawei EulerOS: Security Advisory for python-pip (EulerOS-SA-2024-1020)");
