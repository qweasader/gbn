  if(!isnull(res = isrpmvuln(pkg:"openssl", rpm:"openssl~1.0.2j~1.mga5", rls:"MAGEIA5"))) {
    report += res;
  }

  if(report != "") {
    security_message(data:report);
  } else if(__pkg_match) {
    exit(99);
  }
  exit(0);
