  script_tag(name:"severity_origin", value:"NVD");
  script_tag(name:"severity_date", value:"2021-08-09 16:42:35 +0000 (Mon, 09 Aug 2021)");

  script_name("Huawei EulerOS: Security Advisory for libxslt (EulerOS-SA-2022-2431)");
