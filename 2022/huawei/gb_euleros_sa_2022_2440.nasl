  if(!isnull(res = isrpmvuln(pkg:"java-1.8.0-openjdk-devel", rpm:"java-1.8.0-openjdk-devel~1.8.0.191.b12~0.h31.eulerosv2r7", rls:"EULEROS-2.0SP5"))) {
