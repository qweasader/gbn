  script_tag(name:"vuldetect", value:"Checks if a vulnerable package version is present on the target host.");

  script_tag(name:"insight", value:"Python 3.x through 3.10 has an open redirection vulnerability in lib/http/server.py due to no protection against multiple (/) at the beginning of URI path which may leads to information disclosure.(CVE-2021-28861)");

  script_tag(name:"affected", value:"'python3' package(s) on Huawei EulerOS V2.0SP10(x86_64).");

