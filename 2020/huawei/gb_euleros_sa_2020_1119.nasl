{
  script_oid("1.3.6.1.4.1.25623.1.1.2.2020.1119");
  script_cve_id("CVE-2019-20372");
  script_tag(name:"creation_date", value:"2020-02-24 09:06:54 +0000 (Mon, 24 Feb 2020)");
  script_version("2024-02-05T14:36:56+0000");
  script_tag(name:"last_modification", value:"2024-02-05 14:36:56 +0000 (Mon, 05 Feb 2024)");
  script_tag(name:"cvss_base", value:"4.3");
  script_tag(name:"cvss_base_vector", value:"AV:N/AC:M/Au:N/C:P/I:N/A:N");
  script_tag(name:"severity_vector", value:"CVSS:3.1/AV:N/AC:L/PR:N/UI:N/S:U/C:L/I:N/A:N");
  script_tag(name:"severity_origin", value:"NVD");
  script_tag(name:"severity_date", value:"2020-01-15 15:48:14 +0000 (Wed, 15 Jan 2020)");

  script_name("Huawei EulerOS: Security Advisory for nginx (EulerOS-SA-2020-1119)");
