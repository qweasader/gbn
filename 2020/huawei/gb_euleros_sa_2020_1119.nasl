  script_tag(name:"summary", value:"The remote host is missing an update for the Huawei EulerOS 'nginx' package(s) announced via the EulerOS-SA-2020-1119 advisory.");

  script_tag(name:"vuldetect", value:"Checks if a vulnerable package version is present on the target host.");

  script_tag(name:"insight", value:"NGINX before 1.17.7, with certain error_page configurations, allows HTTP request smuggling, as demonstrated by the ability of an attacker to read unauthorized web pages in environments where NGINX is being fronted by a load balancer.(CVE-2019-20372)");

  script_tag(name:"affected", value:"'nginx' package(s) on Huawei EulerOS V2.0SP5.");

