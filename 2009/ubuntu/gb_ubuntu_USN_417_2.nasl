  script_xref(name:"URL", value:"https://launchpad.net/bugs/83505");

  script_tag(name:"summary", value:"The remote host is missing an update for the 'postgresql-8.1' package(s) announced via the USN-417-2 advisory.");

  script_tag(name:"vuldetect", value:"Checks if a vulnerable package version is present on the target host.");

  script_tag(name:"insight", value:"USN-417-1 fixed several vulnerabilities in the PostgreSQL server.
Unfortunately this update had a regression that caused some valid
queries to be aborted with a type error. This update corrects that
problem.

We apologize for the inconvenience.");

  script_tag(name:"affected", value:"'postgresql-8.1' package(s) on Ubuntu 6.06, Ubuntu 6.10.");

  script_tag(name:"solution", value:"Please install the updated package(s).");

