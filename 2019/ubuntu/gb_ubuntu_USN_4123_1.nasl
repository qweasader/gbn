  script_tag(name:"insight", value:"It was discovered that npm/fstream incorrectly handled certain crafted
tarballs. An attacker could use this vulnerability to write arbitrary files to
the filesystem.");

  script_tag(name:"affected", value:"'node-fstream' package(s) on Ubuntu 18.04, Ubuntu 19.04.");

  script_tag(name:"solution", value:"Please install the updated package(s).");

