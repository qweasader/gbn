include("revisions-lib.inc");
include("pkg-lib-rpm.inc");

release = rpm_get_ssh_release();
if(!release)
  exit(0);

res = "";
report = "";

if(release == "FC20") {
  if(!isnull(res = isrpmvuln(pkg:"texlive", rpm:"texlive~2013~6.20131226_r32488.fc20", rls:"FC20"))) {
    report += res;
  }

  if(report != "") {
    security_message(data:report);
  } else if(__pkg_match) {
    exit(99);
  }
  exit(0);
}

exit(0);
