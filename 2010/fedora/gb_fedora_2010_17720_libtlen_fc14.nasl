  script_cve_id("CVE-2009-3720");
  script_name("Fedora Update for libtlen FEDORA-2010-17720");
  script_tag(name:"summary", value:"The remote host is missing an update for the 'libtlen'
  package(s) announced via the referenced advisory.");
  script_category(ACT_GATHER_INFO);
  script_copyright("Copyright (C) 2010 Greenbone Networks GmbH");
  script_family("Fedora Local Security Checks");
  script_dependencies("gather-package-list.nasl");
  script_mandatory_keys("ssh/login/fedora", "ssh/login/rpms", re:"ssh/login/release=FC14");
  script_tag(name:"affected", value:"libtlen on Fedora 14");
  script_tag(name:"solution", value:"Please install the updated package(s).");
