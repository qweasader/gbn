  script_mandatory_keys("ssh/login/debian_linux", "ssh/login/packages", re:"ssh/login/release=DEB7");

  script_xref(name:"Advisory-ID", value:"DLA-873-1");
  script_xref(name:"URL", value:"https://www.debian.org/lts/security/2017/DLA-873-1");

  script_tag(name:"summary", value:"The remote host is missing an update for the Debian 'apt-cacher' package(s) announced via the DLA-873-1 advisory.");

  script_tag(name:"vuldetect", value:"Checks if a vulnerable package version is present on the target host.");

  script_tag(name:"insight", value:"It was discovered that there was a HTTP response splitting vulnerability in apt-cacher, a proxy server for Debian/Ubuntu software repositories.

For Debian 7 Wheezy, this issue has been fixed in apt-cacher version 1.7.6+deb7u1.

We recommend that you upgrade your apt-cacher packages.");

  script_tag(name:"affected", value:"'apt-cacher' package(s) on Debian 7.");

  script_tag(name:"solution", value:"Please install the updated package(s).");

  script_tag(name:"solution_type", value:"VendorFix");
