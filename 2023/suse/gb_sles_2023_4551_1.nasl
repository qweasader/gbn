    report += res;
  }

  if(!isnull(res = isrpmvuln(pkg:"MozillaFirefox-debuginfo", rpm:"MozillaFirefox-debuginfo~115.5.0~150200.152.117.1", rls:"SLES15.0SP2"))) {
