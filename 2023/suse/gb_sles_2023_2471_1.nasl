  script_tag(name:"severity_origin", value:"NVD");
  script_tag(name:"severity_date", value:"2023-08-29 18:04:00 +0000 (Tue, 29 Aug 2023)");

  script_name("SUSE: Security Advisory (SUSE-SU-2023:2471-1)");
