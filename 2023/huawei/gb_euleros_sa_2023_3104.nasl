  script_tag(name:"severity_origin", value:"NVD");
  script_tag(name:"severity_date", value:"2023-07-27 03:46:00 +0000 (Thu, 27 Jul 2023)");

  script_name("Huawei EulerOS: Security Advisory for pcre2 (EulerOS-SA-2023-3104)");
