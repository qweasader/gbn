  script_tag(name:"last_modification", value:"2023-04-13 10:19:10 +0000 (Thu, 13 Apr 2023)");
  script_tag(name:"creation_date", value:"2023-04-10 01:06:57 +0000 (Mon, 10 Apr 2023)");
  script_name("Fedora: Security Advisory for curl (FEDORA-2023-7e7414e64d)");
  script_category(ACT_GATHER_INFO);
